{
  "rewrites": [
    {
      "type": "prefix",
      "from": "https://go.theregister.com/feed/www.theregister.com",
      "to": "https://www.theregister.com"
    }
  ],
  "rules": [
    {
      "type": "exact",
      "match": "techncruncher.blogspot.com",
      "headers": {
        "Referer": "https://www.google.com/"
      },
      "puppeteer": {
        "javascriptEnabled": false
      },
      "skippedResources": [
        "ads-blocker.app",
        "/rei.php"
      ]
    },
    {
      "type": "suffix",
      "match": "bbc.co.uk",
      "headers": {
        "Referer": "https://www.google.com/"
      },
      "puppeteer": {
        "cookies": [
          {
            "name": "ckns_explicit",
            "value": "1",
            "domain": ".bbc.co.uk",
            "path": "/",
            "secure": true
          },
          {
            "name": "ckns_policy",
            "value": "111",
            "domain": ".bbc.co.uk",
            "path": "/",
            "secure": true
          }
        ]
      }
    }
  ]
}
